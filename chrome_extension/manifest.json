  {
    "name": "Impuesteam - Calculador de impuestos argentinos en STEAM",
    "version": "1.0",
    "description": "Calculador y visualizador de impuestos automatizado para todos los art√≠culos de Steam - Argentina 2020",
    "manifest_version": 2,


	 "content_scripts": [
		{ 
			"matches": ["https://store.steampowered.com/*"],
			"run_at": "document_idle",
			"js": ["js/global_functions.js"],
			"css": ["css/styles.css"]
		},		 
		{
			"matches": ["https://store.steampowered.com/sale/*"],
			"run_at": "document_idle",
			"js": ["js/test_sale.js"]
		},

		{
			"matches": ["https://store.steampowered.com/*"],
			"exclude_matches":["https://store.steampowered.com/sale/*"],
			"run_at": "document_idle",
			"js": ["js/test.js"]
		}
	 ]

  }